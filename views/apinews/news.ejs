<head>
    <link rel="stylesheet" href="/css/apiStyle.css">
</head>

<div class="news-container ">
    <div class="news ">
        <div class="news-box1">
            Dashboard
        </div>
        <% articles.forEach(function(article,index){ %>

        <% if ((typeof article.url=='object' ) || (typeof article.title=='object' ) || (typeof
                article.urlToImage=='object' ) || (typeof article.content=='object' )){ %>
        <% } else{ %>
        <a href="<%- article.url %>" target="_blank" class="news-box Hover-effect">

        <img src="<%- article.urlToImage %>" alt="Image">

        <h3>
            <%- article.title %>
        </h3>


        <p>
            <%- article.content %>
        </p>

        <br>

        <p style="margin-bottom: 0px; position: absolute; right: 0; bottom:font-family ui-monospace">
            Updated: <span style="color:red;"><%- new
            Date(article.publishedAt.slice(0,10)).toDateString() %> <%-
            moment(article.publishedAt).format('hh : mm A');%> 
        </span>
        </p>
                </a>
            <% } %>
        <% }) %>
    </div>
</div>
